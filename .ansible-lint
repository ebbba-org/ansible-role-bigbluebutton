---
# .ansible-lint
# implicit unless exclude_paths is defined in config
exclude_paths:
  - .cache/
  - .github/
warn_list:
  - package-latest
  - jinja[invalid]
  - name[casing]
  - schema[tasks]
  - template-instead-of-copy
  - jinja[spacing]
  - ignore-errors
skip_list:
  - fqcn-builtins
  - internal-error
  - unnamed-task
  - no-changed-when
